<Effects name="Fast Classic">
	<Menus>
		<MenuAppear effect="Fade" duration="100" ease="Out" func="Expo"/>
		<MenuClose effect="Fade" duration="150" ease="In" func="Sine"/>
		<MiniMenuSwap effect="CubeRotateVertical" duration="180" ease="InOut" func="Sine"/>
		<SubMenuAppear effect="SlideHorizontal" duration="100" ease="Out" func="Expo"/>
		<MenuSwap effect="CubeRotateHorizontal" duration="150" ease="InOut" func="Sine"/>
		<SubMenuSwap effect="RotateVertical" duration="150" ease="InOut" func="Sine"/>
		<LockedMenuClose effect="Fade" duration="150" ease="In" func="Sine"/>
	</Menus>
	<Dialogs>
		<Main>
			<Appear effect="Fade" duration="300" ease="Out" func="Back"/>
			<Close effect="Fade" duration="300" ease="In" func="Back"/>
			<Tabs reverse="yes" effect="SlideHorizontal" duration="300" ease="InOut" func="Sine"/>
			<ListView>
				<Open duration="200" ease="InOut" func="Sine"/>
				<GoBack duration="200" ease="InOut" func="Sine"/>
			</ListView>
			<Search effect="Zoom" duration="250" ease="InOut" func="Sine"/>
			<VueSwap effect="RotateHorizontal" duration="200" ease="InOut" func="Sine"/>
			<Tree>
				<Collapse duration="150" ease="Out" func="Pow" param="6"/>
				<Expand duration="150" ease="Out" func="Sine"/>
			</Tree>
			<QuickLaunch>
				<Open duration="450" ease="InOut" func="Sine"/>
				<GoBack duration="450" ease="InOut" func="Sine"/>
			</QuickLaunch>
		</Main>
		<Config>
			<Appear effect="Fade" duration="150" ease="Out" func="Expo"/>
			<MainVue effect="CubeRotateVertical" duration="250" ease="InOut" func="Quad"/>
			<Close effect="Fade" duration="150" ease="In" func="Sine"/>
			<VersionTree>
				<Expand duration="300" ease="Out" func="Sine"/>
				<Collapse duration="300" ease="Out" func="Pow" param="6"/>
			</VersionTree>
		</Config>
		<UpdateInfo>
			<Appear effect="Fade" duration="150" ease="Out" func="Expo"/>
			<Close effect="Fade" duration="150" ease="In" func="Sine"/>
		</UpdateInfo>
		<MenuProperties>
			<Appear effect="Fade" duration="150" ease="Out" func="Expo"/>
			<Close effect="Fade" duration="150" ease="In" func="Sine"/>
		</MenuProperties>
		<OnlineCatalog>
			<Appear effect="Fade" duration="150" ease="Out" func="Expo"/>
			<Close effect="Fade" duration="150" ease="In" func="Sine"/>
		</OnlineCatalog>
		<AppList>
			<Appear effect="Fade" duration="150" ease="Out" func="Expo"/>
			<Close effect="Fade" duration="150" ease="In" func="Sine"/>
		</AppList>
		<AppProperties>
			<Appear effect="Fade" duration="150" ease="Out" func="Expo"/>
			<Close effect="Fade" duration="150" ease="In" func="Sine"/>
		</AppProperties>
		<SetupWizard>
			<Appear effect="Fade" duration="150" ease="Out" func="Expo"/>
			<Close effect="Fade" duration="150" ease="In" func="Sine"/>
		</SetupWizard>
		<UninstallWizard>
			<Appear effect="Fade" duration="150" ease="Out" func="Expo"/>
			<Close effect="Fade" duration="150" ease="In" func="Sine"/>
		</UninstallWizard>
	</Dialogs>
	<ToolTip delay="750">
		<Appear duration="400" ease="Out" func="Expo"/>
		<Move duration="200" ease="InOut" func="Expo"/>
		<Close duration="200" ease="Out" func="Expo"/>
	</ToolTip>
	<InfoBox>
		<Appear duration="250" ease="InOut" func="Circ"/>
		<Close duration="100" ease="InOut" func="Circ"/>
		<Open duration="100" ease="InOut" func="Circ"/>
	</InfoBox>
</Effects>