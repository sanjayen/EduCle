<lbkAppsVCheck version="0.1">
	<UpdateURL>http://update.liberkey.com/v2/</UpdateURL>
	<Effects>
		<Info>
			<FromInfo>Fade</FromInfo>
			<FromApp>RotateLeft</FromApp>
		</Info>
		<AppInfo>
			<FromInfo>RotateRight</FromInfo>
			<FromAppDown>RotateDown</FromAppDown>
			<FromAppUp>RotateUp</FromAppUp>
		</AppInfo>
	</Effects>
	<Language file="LKAppsVCheck_fr.xml">fr</Language>
	<KillTempFolder>yes</KillTempFolder>
</lbkAppsVCheck>